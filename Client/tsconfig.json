{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["es2015", "dom"],
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "typeRoots": ["node_modules/@types"],
        "types": ["jasmine","node"],
        "outDir": "wwwroot",
        "removeComments": true
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}